#include <stdio.h>
#include <string.h>

int main() {
    char text[25];
    
    while(1) {
        printf("Please enter text: ");
        scanf("%s", text);
        
        if(strcmp(text, "Dian") == 0) {
            printf("2002\n");
        }
        else if(strcmp(text, "Quit") == 0) {
            break;
        }
        else {
            printf("Error\n");
        }
    }
    
    return 0;
}
